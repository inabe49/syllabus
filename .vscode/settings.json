{
  "files.exclude": {
    "**/node_modules": true
  },
  "files.eol": "\n",
  "editor.rulers": [
    80
  ],
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "prettier.enable": true,
  "prettier.configPath": ".prettierrc.json",
  "prettier.useEditorConfig": true,
  "cSpell.words": [
  ]
}
